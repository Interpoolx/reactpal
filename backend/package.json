{
    "name": "@reactpress/backend",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "wrangler dev",
        "deploy": "wrangler deploy",
        "build": "echo 'Build handled by wrangler during deployment'",
        "db:push": "npm run db:push --prefix ..",
        "db:studio": "npm run db:studio --prefix ..",
        "db:generate": "npm run db:generate --prefix ..",
        "db:migrate": "node ../scripts/d1.cjs migrations apply --local",
        "db:migrate:remote": "node ../scripts/d1.cjs migrations apply --remote",
        "db:info": "node ../scripts/d1.cjs info --remote",
        "db:backup:local": "node ../scripts/d1.cjs backup --local",
        "db:backup:remote": "node ../scripts/d1.cjs backup --remote",
        "db:restore:local": "node ../scripts/d1.cjs restore --local",
        "db:restore:remote": "node ../scripts/d1.cjs restore --remote"
    },
    "dependencies": {
        "@reactpress/shared": "file:../packages/shared",
        "@reactpress/core-registry": "file:../packages/core-registry",
        "@reactpress/modules-auth": "file:../packages/modules-auth",
        "@reactpress/modules-users": "file:../packages/modules-users",
        "@reactpress/modules-tenants": "file:../packages/modules-tenants",
        "@reactpress/modules-core": "file:../packages/modules-core",
        "@reactpress/modules-cms": "file:../packages/modules-cms",
        "@reactpress/modules-crm": "file:../packages/modules-crm",
        "@reactpress/modules-seo": "file:../packages/modules-seo",
        "hono": "^4.7.2",
        "drizzle-orm": "^0.45.x",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20250109.0",
        "drizzle-kit": "^0.30.2",
        "wrangler": "^3.101.0"
    }
}